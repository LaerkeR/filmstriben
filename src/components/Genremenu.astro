---
import filmgenredatabase from "../filmgenre.json";
import Undermenu from "../layouts/Undermenu.astro";
console.log(filmgenredatabase);

const { navpunkt } = Astro.props;
---

<Undermenu navpunkt="Genre">
  <div class="grid grid-rows-3 grid-cols-5 py-4 gap-x-4 gap-y-2">
    {
      filmgenredatabase.map((genre) => (
        <li class=" py-2 px-2 align-middle">
          <a href={genre}>{genre}</a>
        </li>
      ))
    }
  </div>
</Undermenu>
