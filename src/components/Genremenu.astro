---
import filmgenredatabase from "../filmgenre.json";
import Undermenu from "../layouts/Undermenu.astro";
---

<Undermenu navpunkt="Genre">
  <div class="grid grid-rows-3 grid-cols-6 grid-flow-col py-4 gap-x-4 gap-y-2">
    {
      filmgenredatabase.map((genre) => (
        <li class=" py-1 px-1 align-middle">
          <a class="Underline" href={genre}>
            {genre}
          </a>
        </li>
      ))
    }
  </div>
</Undermenu>
